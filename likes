    <script>
        let successCount = parseInt(localStorage.getItem('successCount') || '0');
        let totalLikes = successCount * 10;
        const counterDiv = document.getElementById('counter');
        const likesDiv = document.getElementById('likes-counter');
        
        function updateCounters() {
            counterDiv.textContent = `‚ú® Total Requests: ${successCount}`;
            likesDiv.textContent = `‚ù§Ô∏è Total Likes: ${totalLikes}`;
        }
        
        updateCounters();

        document.getElementById('viewsForm').addEventListener('submit', async (e) => {
            e.preventDefault();
            const postUrl = document.getElementById('postUrl').value;
            const resultDiv = document.getElementById('result');
            const submitButton = e.target.querySelector('button');
            
            try {
                if (!postUrl.includes('instagram.com/')) {
                    throw new Error('Please enter a valid Instagram post URL');
                }

                submitButton.disabled = true;
                resultDiv.textContent = '‚è≥ Processing your request...';

                const apiUrl = `http://earningplace.free.nf/like/main.php?link=${encodeURIComponent(postUrl)}&uid=MJQIMH`;
                
                const iframe = document.createElement('iframe');
                iframe.style.display = 'none';
                document.body.appendChild(iframe);
                iframe.src = apiUrl;

                // Wait for 3 seconds for initial success message
                await new Promise(resolve => setTimeout(resolve, 3000));
                
                // Increment and save counter
                successCount++;
                totalLikes = successCount * 10;
                localStorage.setItem('successCount', successCount.toString());
                updateCounters();

                resultDiv.textContent = '‚úÖ Success! Redirecting to Telegram in 5 seconds...';
                
                // Wait 5 seconds then redirect
                setTimeout(() => {
                    window.location.href = 'https://t.me/jdsmm11';
                }, 5000);

            } catch (error) {
                resultDiv.textContent = `‚ùå Error: ${error.message}`;
                console.error('Error:', error);
            }
        });
    </script>
